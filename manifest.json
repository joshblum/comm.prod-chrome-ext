{
    "manifest_version": 2,
    "name" : "comm.prod",
    "version" : "0.0.3",
    "description" : "Get current comm.prods in your browser.",
    "browser_action" : {
        "default_icon" : "img/bomb.png",
        "default_popup" : "popup.html",
        "default_title" : "comm.prod"
    },
    "permissions": [
        "http://burtonthird.com/.*",
        "http://localhost:8000/.*",
        "cookies",
        "tabs",
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "background" : {
            "page": "background.html"
    },
    "icons": {
           "48": "img/btb-logo-48.png",
          "128": "img/btb-logo-128.png" 
    }
}
